{
  "name": "vue-starter",
  "type": "module",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@8.1.0",
  "scripts": {
    "dev": "vite --port 3000",
    "build": "run-p type-check build-only && cp dist/index.html dist/200.html",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "deploy:vercel": "npx vercel",
    "deploy:ntl": "npx ntl deploy",
    "deploy:surge": "npx surge dist"
  },
  "dependencies": {
    "@vueuse/core": "^10.6.0",
    "pinia": "^2.1.7",
    "vue": "3.3.8",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^1.1.1",
    "@iconify-json/fluent-emoji": "^1.1.15",
    "@iconify-json/ph": "^1.1.6",
    "@matijaoe/eslint-config": "^1.1.0",
    "@rushstack/eslint-patch": "^1.5.1",
    "@types/node": "^20.9.0",
    "@unocss/eslint-plugin": "^0.57.3",
    "@vitejs/plugin-vue": "^4.4.1",
    "eslint": "^8.53.0",
    "npm-run-all": "^4.1.5",
    "pnpm": "^8.10.3",
    "postcss": "^8.4.31",
    "postcss-nesting": "12.0.1",
    "typescript": "~5.2.2",
    "unocss": "^0.57.3",
    "unplugin-auto-import": "^0.16.7",
    "unplugin-vue-components": "^0.25.2",
    "unplugin-vue-router": "^0.7.0",
    "vite": "^4.5.0",
    "vue-tsc": "^1.8.22"
  }
}
