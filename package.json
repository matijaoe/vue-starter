{
  "name": "vue-starter",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@8.1.0",
  "scripts": {
    "dev": "vite --port 3000",
    "build": "run-p type-check build-only && cp dist/index.html dist/200.html",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore --fix",
    "deploy:vercel": "npx vercel",
    "deploy:ntl": "npx ntl deploy",
    "deploy:surge": "npx surge dist"
  },
  "dependencies": {
    "@vueuse/core": "^10.3.0",
    "pinia": "^2.1.6",
    "vue": "3.3.4",
    "vue-router": "^4.2.4"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.40.0",
    "@iconify-json/fluent-emoji": "^1.1.13",
    "@iconify-json/ph": "^1.1.5",
    "@rushstack/eslint-patch": "^1.3.2",
    "@types/node": "^20.4.7",
    "@vitejs/plugin-vue": "^4.2.3",
    "eslint": "^8.46.0",
    "npm-run-all": "^4.1.5",
    "pnpm": "^8.6.11",
    "postcss": "^8.4.27",
    "postcss-nesting": "12.0.0",
    "typescript": "~5.1.6",
    "unocss": "^0.54.1",
    "unplugin-auto-import": "^0.16.6",
    "unplugin-vue-components": "^0.25.1",
    "unplugin-vue-router": "^0.6.4",
    "vite": "^4.4.8",
    "vue-tsc": "^1.8.8"
  }
}
