{
  "name": "vue-starter",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@8.1.0",
  "scripts": {
    "dev": "vite --port 3000",
    "build": "run-p type-check build-only && cp dist/index.html dist/200.html",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore --fix",
    "deploy:vercel": "npx vercel",
    "deploy:ntl": "npx ntl deploy",
    "deploy:surge": "npx surge dist"
  },
  "dependencies": {
    "@vueuse/core": "^9.13.0",
    "pinia": "^2.0.34",
    "vue": "3.3.0-alpha.9",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.38.4",
    "@iconify-json/fluent-emoji": "^1.1.13",
    "@iconify-json/ph": "^1.1.5",
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/node": "^18.15.11",
    "@vitejs/plugin-vue": "^4.1.0",
    "eslint": "^8.38.0",
    "npm-run-all": "^4.1.5",
    "pnpm": "^8.1.1",
    "postcss": "^8.4.21",
    "postcss-nesting": "^11.2.2",
    "typescript": "~5.0.4",
    "unocss": "^0.50.8",
    "unplugin-auto-import": "^0.15.2",
    "unplugin-vue-components": "^0.24.1",
    "unplugin-vue-router": "^0.5.4",
    "vite": "^4.2.1",
    "vue-tsc": "^1.2.0"
  }
}
